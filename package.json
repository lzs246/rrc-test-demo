{
  "name": "rrc-test",
  "appGroupName": "rrc-test",
  "main": "hel_proxy/entry.js",
  "module": "hel_proxy_es/entry.js",
  "types": "lib/entrance/libTypes.ts",
  "unpkg": "hel_dist/entry.js",
  "version": "1.0.1",
  "dependencies": {
    "hel-lib-proxy": "^3.8.0",
    "hel-micro": "^3.8.1",
    "react": ">=16.14.0",
    "react-app-polyfill": ">=1.0.6",
    "react-dom": ">=16.14.0"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-app-polyfill": ">=1.0.6",
    "react-dom": ">=16.14.0"
  },
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "check": "node ./scripts/check.js",
    "start": "cross-env PORT=3103 node scripts/start.js",
    "build": "npm run check_name && npm run build_dist && npm run build_meta && npm run build_proxy",
    "check_name": "node scripts/check.js",
    "build_dist": "node scripts/build.js",
    "build_proxy": "npm run build_proxy_umd && npm run build_proxy_es && cp ./hel_proxy/entry.js ./hel_dist",
    "build_proxy_umd": "tsc & node ./scripts/replaceToRelativePath.js & rollup -c",
    "build_proxy_es": "tsc & node ./scripts/replaceToRelativePath.js & cross-env BUILD_ENV=es rollup -c",
    "lint": "eslint ./src --ext ts,tsx",
    "lintfix": "eslint ./src --ext ts,tsx --fix",
    "build_meta": "node scripts/meta.js",
    "set_home_page": "cross-env-shell HEL_APP_HOME_PAGE=http://127.0.0.1:8080",
    "build_cust": "npm run set_home_page \"npm run build_dist && npm run build_meta\"",
    "test": "jest",
    "snap": "jest -u"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@svgr/webpack": "4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "ajv": "^6.12.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "hel-dev-utils": "^2.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "http-proxy-middleware": "^1.0.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
